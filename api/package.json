    "url": "https://github.com/Ekown/grocery-tracker/issues"
  },
  "homepage": "https://github.com/Ekown/grocery-tracker#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.1",
    "sequelize-cli": "^6.5.1",
    "sequelize-cockroachdb": "^6.0.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0"
